AT_BANNER([[Eulerian paths and cycles]])

AT_SETUP([Is eulerian (igraph_is_eulerian):])
AT_KEYWORDS([igraph_is_eulerian eulerian])
AT_COMPILE_CHECK([tests/igraph_is_eulerian.c], [tests/igraph_is_eulerian.out])
AT_CLEANUP

AT_SETUP([Find eulerian cycle (igraph_eulerian_cycle):])
AT_KEYWORDS([igraph_eulerian_cycle eulerian_cycle])
AT_COMPILE_CHECK([tests/igraph_eulerian_cycle.c], [tests/igraph_eulerian_cycle.out])
AT_CLEANUP

AT_SETUP([Find eulerian path (igraph_eulerian_path):])
AT_KEYWORDS([igraph_eulerian_path eulerian_path])
AT_COMPILE_CHECK([tests/igraph_eulerian_path.c], [tests/igraph_eulerian_path.out])
AT_CLEANUP